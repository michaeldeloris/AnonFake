<% String title = (String) request.getAttribute("page_title"); %>

<div class="header">
  <div class="header_logo">
    <a class="logo_link" href="./">
      <img class="logo"src="images/anonfake_logo.png">
    </a>
  </div>
  <% if(title != null && !title.isEmpty()) { %>
    <div class="header_title">
      <%= title %>
    </div>
  <% } %>
</div>

